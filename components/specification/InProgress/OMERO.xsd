		<xsd:restriction base="OME:LSID">
			<xsd:pattern value="(urn:lsid:([\w\-\.]+\.[\w\-\.]+)+:OriginalFile:\S+)|(OriginalFile:\S+)"/>
		</xsd:restriction>
	</xsd:simpleType>

